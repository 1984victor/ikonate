{
  "name": "custom-icons",
  "version": "0.2.0",
  "description": "fully customisable svg icons",
  "keywords": [
    "icons",
    "set",
    "outline",
    "customisable"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eucalyptuss/custom-icons.git"
  },
  "scripts": {
    "sprite:dir": "node scripts/spritesDir.js",
    "svgSpriteGenerate": "svg-sprite-generate -d ./icons -o ./sprite/custom-icons.svg",
    "generate:sprite": "node -e \"require('./scripts/createSvgSpritesheet.js')()\"",
    "generate:html": "node -e \"require('./scripts/generateHTML')()\"",
    "demo": "npm run generate:sprite save && npm run generate:html sprite save",
    "build": "run-s demo sprite:**"
  },
  "author": "Mikolaj Dobrucki <tease@mikolajdobrucki.com>",
  "license": "MIT",
  "devDependencies": {
    "jsdom": "11.10.0",
    "npm-run-all": "^4.1.3",
    "pretty": "^2.0.0",
    "svg-sprite-generator": "0.0.7",
    "xmldom": "^0.1.27",
    "minimist": "^1.2.0"
  }
}
